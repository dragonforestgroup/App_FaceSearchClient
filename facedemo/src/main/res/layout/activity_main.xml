<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_face_register"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="人脸注册" />

    <Button
        android:id="@+id/btn_face_recognize"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="人脸识别" />


    <Button
        android:id="@+id/btn_face_img_upload_register"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="图像人脸注册,图像上传" />

    <Button
        android:id="@+id/btn_face_feature_upload_register"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="特征码人脸注册,图像上传" />


    <Button
        android:id="@+id/btn_face_img_upload_recognize"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="图像人脸识别,图像上传" />

    <Button
        android:id="@+id/btn_face_feature_upload_recognize"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="特征码人脸识别,图像上传" />

    <Button
        android:id="@+id/btn_face_register_and_recognize"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="人脸识别和优化" />

    <Button
        android:id="@+id/btn_face_img_upload_dayou"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="人脸识别,图像上传,大有test" />
</LinearLayout>
